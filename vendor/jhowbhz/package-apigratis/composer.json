{
    "name": "jhowbhz/package-apigratis",
    "description": "Uma forma simples de consumir serviÃ§os gratuitos oferecidos pelo site https://apigratis.com.br/",
    "keywords": [
        "apigratis",
        "whatsapp",
        "messages",
        "api",
        "myzap",
        "messages free",
        "php requests"
    ],
    "homepage": "https://apigratis.com.br",
    "authors": [
        {
            "name": "Jhon",
            "email": "contato@apigratis.com.br",
            "homepage": "https://apigratis.com.br",
            "role": "Developer"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "composer/composer": "^2.2.0",
        "php": ">=7.4"
    },
    "require-dev": {
        "php": ">=7.4",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "ApiGratis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiGratis\\Test\\": "tests"
        }
    },
    "suggest": {
        "ext-curl": "Required by ApiGratis\\ to requests in services."
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
